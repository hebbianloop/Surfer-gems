	Number = {3},
	Pages = {2264--2274},
	Pmid = {19874902},
	Shorttitle = {Atrophy patterns in Alzheimer's disease and semantic dementia},
	Title = {Atrophy patterns in Alzheimer's disease and semantic dementia: a comparison of {FreeSurfer} and manual volumetric measurements},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/19874902},
