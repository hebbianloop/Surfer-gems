	Journal = {Ann N Y Acad Sci},
	Keywords = {Adult, Aged, Aged, 80 and over, Aging/*pathology, Atrophy/*diagnosis/physiopathology, Brain Mapping/*methods, Dementia/*diagnosis/physiopathology, Diagnosis, Differential, Diffusion Magnetic Resonance Imaging/*methods/trends, Humans, Middle Aged, Nerve Fibers, Myelinated/*pathology, Predictive Value of Tests, Prefrontal Cortex/*pathology/physiopathology},
