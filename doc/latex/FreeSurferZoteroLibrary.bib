	Issn = {1065-9471},
	Journal = {Human brain mapping},
	Month = jan,
	Number = {1},
	Pages = {175--184},
	Pmcid = {PMC2883170},
	Pmid = {17985349},
	Title = {Abnormal Cerebral Cortex Structure in Children with {ADHD}},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2883170/},
	Urldate = {2014-08-23},
