	Journal = {{NeuroImage}},
	Keywords = {Cerebral Cortex, Cognition Disorders, Early Diagnosis, Female, Hepatic Encephalopathy, Humans, Image Interpretation, Computer-Assisted, Magnetic Resonance Imaging, Male, Middle Aged, Neuropsychological Tests},
	Language = {eng},
