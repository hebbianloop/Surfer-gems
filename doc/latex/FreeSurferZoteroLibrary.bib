	Language = {eng},
	Month = jun,
	Number = {2},
	Pages = {572--584},
	Pmid = {16503170},
	Title = {A novel quantitative cross-validation of different cortical surface reconstruction algorithms using {MRI} phantom.},
	Url = {https://surfer.nmr.mgh.harvard.edu/ftp/articles/lee_j_31p572_2006.pdf},
	Volume = {31},
	Year = {2006},
	Bdsk-Url-1 = {https://surfer.nmr.mgh.harvard.edu/ftp/articles/lee_j_31p572_2006.pdf},
