@article{makris_human_2003,
	Abstract = {We describe a system of surface-assisted parcellation ({SAP}) of the human cerebellar cortex derived from neural systems functional and behavioral anatomy. This system is based on {MRI} and preserves the unique morphologic and topographic features of the individual cerebellum. All major fissures of the cerebellum were identified and traced in the flattened representation of the cerebellar cortex using the program "{FreeSurfer}." Parcellation of the cerebellar cortex followed using the fissure information in conjunction with landmarks using the program "Cardviews" to create 64 gyral-based cerebellar parcellation units. Computer-assisted algorithms enable the execution of the cerebellar parcellation procedure as well as volumetric measurements and topographic localization. The {SAP} technique makes it possible to represent multimodal structural and functional imaging data on the flattened surface of the cerebellar cortex as illustrated in one functional {MRI} experiment.},
	Annote = {Makris, {NikosHodge}, Steven {MHaselgrove}, {ChristianKennedy}, David {NDale}, {AndersFischl}, {BruceRosen}, Bruce {RHarris}, {GordonCaviness}, Verne S {JrSchmahmann}, Jeremy {DengNS}34189/{NS}/{NINDS} {NIH} {HHS}/{NS}39581/{NS}/{NINDS} {NIH} {HHS}/P01 03610/{PHS} {HHS}/{RR}16594-01A1/{RR}/{NCRR} {NIH} {HHS}/Research Support, Non-U.S. Gov'{tResearch} Support, U.S. Gov't, P.H.S.2003/06/14 05:00J Cogn Neurosci. 2003 May 15;15(4):584-99.},
