	Keywords = {Adult, {CADASIL}, Cerebral Cortex, Cerebral Infarction, Female, Follow-Up Studies, Humans, Image Processing, Computer-Assisted, Incidence, Leukoencephalopathies, Magnetic Resonance Imaging, Male, Middle Aged, Nerve Fibers, Myelinated, Oxygen, Prospective Studies, Statistics, Nonparametric, Time Factors},
	Language = {eng},
