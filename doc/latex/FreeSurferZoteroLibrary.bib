	Language = {eng},
	Number = {4},
	Pages = {1263--1272},
	Pmid = {20061613},
	Title = {Use of {SVM} methods with surface-based cortical and volumetric subcortical measurements to detect Alzheimer's disease},
	Url = {https://surfer.nmr.mgh.harvard.edu/pub/articles/Oliveira_JAD2010.pdf},
