	Journal = {{NeuroImage}},
	Keywords = {Algorithms, Brain, Female, Gestational Age, Humans, Image Processing, Computer-Assisted, Infant, Newborn, Magnetic Resonance Imaging, Male, Observer Variation, Posture, Reproducibility of Results},
