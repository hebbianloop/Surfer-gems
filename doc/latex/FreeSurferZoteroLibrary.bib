	Journal = {The American journal of psychiatry},
	Month = jul,
	Number = {7},
	Pmcid = {PMC3815681},
	Pmid = {22581200},
	Title = {Cortical thinning in psychopathy},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3815681/},
	Urldate = {2014-08-23},
	Volume = {169},
	Year = {2012},
