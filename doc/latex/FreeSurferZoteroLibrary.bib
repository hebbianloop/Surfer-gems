	Language = {eng},
	Month = jan,
	Number = {1},
	Pages = {37--46},
	Pmid = {18494772},
	Title = {Comparative reliability of total intracranial volume estimation methods and the influence of atrophy in a longitudinal semantic dementia cohort},
	Url = {https://surfer.nmr.mgh.harvard.edu/ftp/articles/pengas_g_inpress_2008.pdf},
