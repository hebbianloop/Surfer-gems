	File = {Polimeni-2010-Laminar analysis of 7T BOLD usin:/autofs/cluster/freesurfer/zotero/storage/2Q45VCJI/Polimeni-2010-Laminar analysis of 7T BOLD usin.pdf:application/pdf;PubMed Central Full Text PDF:/autofs/cluster/freesurfer/zotero/storage/TQMX4RNB/Polimeni et al. - 2010 - Laminar analysis of 7 T BOLD using an imposed spat.pdf:application/pdf},
