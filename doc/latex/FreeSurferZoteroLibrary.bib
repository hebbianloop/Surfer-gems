	Month = jan,
	Number = {1},
	Pages = {70--80},
	Shorttitle = {Automated manifold surgery: constructing geometrically accurate and topologically correct models of the human cerebral cortex},
	Title = {Automated manifold surgery: constructing geometrically accurate and topologically correct models of the human cerebral cortex},
