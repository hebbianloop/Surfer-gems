	Keywords = {Adolescent, Adult, Bipolar Disorder, Brain, Cerebral Cortex, Comorbidity, Depressive Disorder, Major, Dominance, Cerebral, Female, Hippocampus, Humans, Image Processing, Computer-Assisted, Imaging, Three-Dimensional, Male, Mental Disorders, Middle Aged, Psychiatric Status Rating Scales, Psychotic Disorders, Reference Values, Schizophrenia, Schizotypal Personality Disorder, Siblings, Software},
