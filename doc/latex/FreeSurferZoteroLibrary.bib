	Shorttitle = {Automatic classification of patients with Alzheimer's disease from structural {MRI}},
	Title = {Automatic classification of patients with Alzheimer's disease from structural {MRI}: a comparison of ten methods using the {ADNI} database},
	Url = {http://cogimage.dsi.cnrs.fr/perso/colliot/files/neuroimage2010_Remi_Comparaison.pdf},
