	Doi = {10.1016/j.neuroimage.2006.02.051},
	Issn = {1053-8119 (Print) 1053-8119 (Linking)},
	Journal = {Neuroimage},
	Keywords = {Aged, Aged, 80 and over, Brain Mapping, Cerebral Cortex/*anatomy \& histology, fs\_Thickness-measurement, Functional Laterality, Humans, Image Processing, Computer-Assisted, Magnetic Resonance Imaging/*methods, Reference Values, Reproducibility of Results},
