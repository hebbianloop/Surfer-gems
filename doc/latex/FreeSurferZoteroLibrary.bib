	Author = {Fennema-Notestine, C. and Panizzon, M. S. and Thompson, W. R. and Chen, C. H. and Eyler, L. T. and Fischl, B. and Franz, C. E. and Grant, M. D. and Jak, A. J. and Jernigan, T. L. and Lyons, M. J. and Neale, M. C. and Seidman, L. J. and Tsuang, M. T. and Xian, H. and Dale, A. M. and Kremen, W. S.},
	Doi = {10.3233/JAD-2011-0002},
