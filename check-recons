################################################################################################################
################################################################################################################
################################################################################################################
######## PARSE OPTIONS
########
########
########
################################################################################################################
################################################################################################################
function check-recons_parseopts(){
	# defs
	check-recons_defaults
	if [ "$#" = 0 ]; then
		check-recons_usage
		exit
	fi
	# Parse Options
	while :; do
	    case ${1} in
		--no-colors|--plain|-nc)
			PRETTY='plainjane' && check-recons_defaults
			;;		    	
	    --status)
		    if [ -n "$2" ]; then
		    	if [ "$2" = 'active' ]; then
		    		STATUS+=("${GREY}${2}${NC}")
		    	elif [ "$2" = 'done' ]; then
		    		STATUS+=("${GREEN}${2}${NC}")
		    	elif [ "$2" = 'failed' ]; then
		    		STATUS+=("${YELLOW}${2}${NC}")
		    	else
		    		echo -e ${BRED}'ERROR: '${NC}'legal values for --status are "active" "done" "failed". User input was "'${2}'"\n...exiting'
		    	fi
				shift
			else
				STATUS=('active' 'done' 'failed')
		    fi
		    ;;
	    -v|--verbose)
			VERBOSE='givemeinfo'
		    if [ -n "$2" ]; then
		    	if [ "$2" = 'active' ]; then
		    		STATUS+=("${GREY}${2}${NC}")
		    	elif [ "$2" = 'done' ]; then
		    		STATUS+=("${GREEN}${2}${NC}")
		    	elif [ "$2" = 'failed' ]; then
		    		STATUS+=("${YELLOW}${2}${NC}")
		    	else
		    		echo -e ${BRED}'ERROR: '${NC}'legal values for --status are "active" "done" "failed". User input was "'${2}'"\n...exiting'
		    	fi
				shift
			else
				STATUS=('active' 'done' 'failed')
		    fi		
		    ;;			    
		--convert)
			CONVERTFILES='convertintoNIFTI'
			;;		
	    -f|--files)
			FILECHECK='checkemall'
		    ;;		    		    	    		    
		--web-report)
		    if [ -n "$2" ]; then
				STATUS+=("${2}")
				shift
		    fi
			HTMLREPORT='makereport'
			;;
		--web-report-dir|-wrd)
		    if [ -n "$2" ]; then
				REPORTDIR="${2}"
				shift
			else
				echo -e "${BRED}ERROR: ${NC}--web-report-dir -wrd requires a non empty option argument.\n" >&2
				exit				
		    fi
			HTMLREPORT='makereport'
			;;			
	    --detail)
			RECONSTATS='getdetails'
		    ;;		
	    --kill-jobs)
			KILLJOBS='stopActiveSGEJobs'
		    ;;
		-h|--help|?|--usage)
			check-recons_usage
			exit
			;;
		-sd|--subjects-dir)
		    if [ -n "$2" ]; then
				SUBJECTS_DIR=${2}
				shift
		    else
				echo -e "${BRED}ERROR: ${NC}-sd --subjects-dir requires a non empty option argument.\n" >&2
				exit
		    fi
		    ;;
		-s|--subjects)
		    if [ -n "$2" ]; then
				SUBJECTS=${2}
				shift
		    else
				echo -e "${BRED}ERROR: ${NC}-s --subjects requires a non empty option argument.\n" >&2
				exit
		    fi
		    ;;
		?*)
		    printf '\n'${YELLOW}' Warning: '${NC}'Unknown option: "%s"\n..exiting' "${1}" >&2
		    exit
		    ;;
		*)
		    break
	    esac
	    shift
	done
}
function check-recons_dependencies(){
	if [ $(uname) = 'Darwin' ]; then
		# check for coreutils
		if [ ! $(brew list | grep 'coreutils') ]; then
			brew install coreutils
			export PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"
			export MANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"
			echo -e '\n#add coreutils to path\nexport PATH="/usr/local/opt/coreutils/libexec/gnubin:${PATH}"\nMANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"\n'  >> ${HOME}/.bash_profile
		fi
		# check date in path is coreutils date
		if [ ! "$(date --version 2&> /dev/null | grep 'GNU coreutils')" ]; then
			export PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"
			export MANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"
		fi					
	fi
}
################################################################################################################
################################################################################################################
################################################################################################################
######## TOP LEVEL FLOW CONTROL
########
########
########
################################################################################################################
################################################################################################################
function check-recons(){
	# check dependencies
	check-recons_dependencies
	# parse user input
	check-recons_parseopts "$@"
	# defs
	check-recons_defaults	
	# get location of program
	check-recons_whereami	
	# convert files if user asks
	[ "${CONVERTFILES}" ] && check-recons_convert.NIFTI
	# kill SGE jobs, delete isRunning files
	## TODO - add ability to kill by pid (need intelligent ps aux matching)
	[ "${KILLJOBS}" ] && check-recons_killJobs
	# check file manifest of each subject
	[ "${FILECHECK}" ] && check-recons_files && check-recons_printstatus
	# check status of active/failed/done
	[ "${STATUS[*]}" ] && [ ! "${FILECHECK}" ] && check-recons_status && check-recons_printstatus || [ "${VERBOSE}" ] && check-recons_printstatus_verbose
	# get details of reconstruction statistics
	[ "${RECONSTATS}" ] && check-recons_stats && check-recons_table
	# make HTML webreport 
	[ "${HTMLREPORT}" ] && check-recons_html && open ${REPORTDIR}/index.html
	# [ ! -z "${RECONSTATS[*]}" ] && check-recons_buildtable
	## PERFORM DIFF BETWEEN DIRS AND SUBJECTS WITH LOG FILES??
}
check-recons "$@"
echo ""
echo "check-recon completed the task in ${SECONDS} seconds"
