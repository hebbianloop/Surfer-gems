function check-recons_html(){
	echo -e "${BLUE}Building Web-Report${NC}"
	if [ ! ${SUBJECTS} ]; then
		check-recons_status
		SUBJECTS=("${RUNNINGSUBJECTS[@]}" "${FINISHEDSUBJECTS[@]}" "${FAILEDSUBJECTS[@]}")		
	fi
	check-recons_html.init
	# resources & references 
	# http://www.compciv.org/topics/web/heredocs_and_html/
	# http://www.compciv.org/topics/bash/text-values/#heredocs-info
	# https://stackoverflow.com/questions/1167746/how-to-assign-a-heredoc-value-to-a-variable-in-bash
	# https://www.mathworks.com/matlabcentral/fileexchange/40942-plot-mesh-as-interactive-html?s_tid=gn_loc_drop
	# Build index programatically from beginning to end
	# Include a separate lightbox for each subject
	# Take screenshots of each subject brainmask // white matter // pial surface overlays
	#	- include GIFS of rotating surface?
	#for SUBJECT in ${SUBJECTS[@]}; do
	#	echo ''
	#done
	# Later...
	# Include graphics for summary report of data from subject
}
function check-recons_html.sync(){
	local CWD=$(pwd)
	[ -d ${REPORTDIR} ] && cd ${REPORTDIR} && git pull && cd ${CWD}
}
function check-recons_html.publish(){
	local CWD=$(pwd)
	if [ -d ${REPORTDIR} ]; then
		cd ${REPORTDIR}
		if git commit -am "web update $(hostname)"; then
			if git push; then
				cd ${CWD}	
			fi
		fi
	fi
}
function check-recons_html.papaya(){
	local subject=${1} && local images=${2} && local cwd=$(pwd)
	cd ${REPORTDIR}/assets/papaya && ${REPORTDIR}/assets/papaya/papaya-builder.sh -images ${images} -local
	cp ${REPORTDIR}/assets/papaya/build/papaya.js ${REPORTDIR}/assets/js/${subject}.papaya.js && cd ${cwd}
}
function check-recons_html.init(){
	# get all jobs
	# Clone Report Tools If You Don't Have Them
	if [ ! -d ${REPORTDIR} ]; then
		git clone ${REPORTREPO_TEMPLATE} ${REPORTDIR}
	fi
	# Initialize INDEX
	PAGETITLE='Surfer-gems | Web Reports'
	PROJECTNAME='Demo FreeSurfer Project'
	HEADERNAME='FreeSurfer Project: Web Reports'
	DESCRIPTION='🤵🏻 Automated web reports of 3D structural MR images reconstructed with the <a href="http://freesurfer.net"> FreeSurfer software package</a>. Generated with <a href="https://github.com/seldamat/Surfer-gems">Surfer-gems</a>, <a href="https://github.com/rii-mango/Papaya.git">Papaya</a> and <a href="https://html5up.net">html5up</a>'	
	check-recons_html.init.index
	# Initialize Landing Pages
	## - TODO clean this up.. make it more customizable 
	## - TODO configure with default but allow user modification
	## - TODO make TILEPAGES dynamic and configurable via append function
	## group
	PAGETITLE=${PROJECTNAME}'| FreeSurfer Average Subject Report'
	HEADERNAME=${PROJECTNAME}': Group Averaged Reports'
	DESCRIPTION='Select from different group summaries'	
	TILES="atlas thickness curvature surface-area volumes edits contrast-to-noise"
	TILEPICS=("tile.average.atlas" "tile.average.thickness" "tile.average.curvature" "tile.average.surface-area" "tile.average.tpm" "tile.average.edits" "tile.average.contrast-to-noise")
	TILEPAGES=("group.atlas" "group.thickness" "group.curvature" "group.surface-area" "group.tpm" "group.edits" "group.contrast-to-noise")	
	HTMLFILE=${REPORTDIR}/pages/group.landing.html	
	check-recons_html.init.landing ${HTMLFILE}
	## create subject index landing page with nested list of data view types (convergent recursion)
	PAGETITLE=${PROJECTNAME}'| Subject Index'
	HEADERNAME=${PROJECTNAME}': Subject Index'	
	DESCRIPTION='Browse brain volumes, surfaces, summary reports and statistics'			
	TILES=("${SUBJECTS[@]}")
	for TILE in "${TILES[@]}"; do
		TILEPAGES+=("indiv.landing.${TILE}")
		TILEPICS+=("tile."${TILE})
		TILEDESCRIPTIONS+=("(Age, Sex)")		
	done
	HTMLFILE=${REPORTDIR}/pages/indiv.landing.html
	check-recons_html.init.landing ${HTMLFILE} ${TILEDESCRIPTIONS}
	### create nested landings 
	TILEPAGES=() && TILEPICS=()
	for SUBJECT in "${SUBJECTS[@]}"; do
		TILES=("atlas" "surface-morphometrics" "edits" "contrast-to-noise")
		HEADERNAME=' Surface &amp; Volume Summaries: '${SUBJECT}
		for TILE in "${TILES[@]}"; do
			TILEPAGES+=("${SUBJECT}.${TILE}")
			TILEPICS+=("tile.${TILE}.${LANDING}")
			check-recons_html.init.tile ${TILE} "${REPORTDIR}/pages/${SUBJECT}.${TILE}.html" ${SUBJECT}
			# generate data viewer page (papaya or 3d render somehow or screenshots in carousel)
			# check-recons_html.init.summary ${REPORTDIR}/pages/indiv.${LANDING}.${TILE}.summary.html
		done 		
		check-recons_html.init.landing ${REPORTDIR}/pages/indiv.landing.${SUBJECT}.html "${TILES}"
	done	


}
function check-recons_html.init.tile(){
	local tile="${1}" && local outfile=${2} && local subject=${3} && local images=""
	if [ "${tile}" = 'atlas' ]; then
		check-recons_convert.NIFTI
		searchfor='T1 brainmask wm aseg aparc+aseg aparc.DKTatlas+aseg aparc.a2009s+aseg wmparc'
		found=""
		for search in ${searchfor}; do
			found+="$(find ${SUBJECTS_DIR}/${subject} -type f -name "${search}.nii.gz") "
		done
		check-recons_html.papaya "${subject}" "${found}" 
		check-recons_html.init.tile.papaya "$(echo ${searchfor} | sed 's|+|_|g' | sed 's|\.|_|g')" "${outfile}" ${subject}
	# elif [ $(echo "${tile}" | grep 'surface') ]; then
	# 	echo check-recons_html.init.tile.surface
	# elif [ "${tile}" = 'edits' ]; then
	# 	echo ''
	# elif [ "${tile}" = 'contrast-to-noise' ]; then
	# 	echo ''
	# else
	# 	echo ''
	fi

}
