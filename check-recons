### DEFINE FUNCTION
##  Usage info
function check-recons_usage(){
read -r -d '' HELPMESSAGE <<EOF
\n\033[33;5m✨\033[0m${GREY}The Magical Surfer-gem Box${BLUE}\033[33;5m✨\033[0m\n\n

This program contains a bounty of gems for working with FreeSurfer Datasets. 

\n\n ■ Summarize all jobs in SUBJECTS_DIR
\n\t >> ${0##*/} --status
\n\t >> ${0##*/} -h
\n\t >> ${0##*/} -u
\n\t >> ${0##*/} --help
\n\t >> ${0##*/} --usage

\n\n
 ■ EXAMPLE:
 	${0##*/} -sd ${SUBJECTS_DIR}\n
 	\t\t⤷ 


\n\n ■ Getting Help
\n\t >> ${0##*/}
\n\t >> ${0##*/} -h
\n\t >> ${0##*/} -u
\n\t >> ${0##*/} --help
\n\t >> ${0##*/} --usage

EOF
echo -e ${HELPMESSAGE} | fold -w 80 -s 
}
### DEFINE FUNCTION
## figure out where function is being called from (follow symlinks)
function check-recons_whereami(){
	while [ -h "${SOURCE}" ]; do # resolve $SOURCE until the file is no longer a symlink
		DIR="$( cd -P "$( dirname "${SOURCE}" )" && pwd )"
		SOURCE="$(readlink "${SOURCE}")"
		[[ ${SOURCE} != /* ]] && SOURCE="${DIR}/${SOURCE}" # if $SOURCE was a relative symlink, we need to resolve it relative to the path where the symlink file was located
	done
	DIR="$( cd -P "$( dirname "${SOURCE}" )" && pwd )"	
	# get where this script lives
	SOURCEPATH="`dirname \"$0\"`"              # relative
	SOURCEPATH="`( cd \"${SOURCEPATH}\" && pwd )`"	
}
### DEFINE FUNCTION
##  Preallocate arrays and default values
function check-recons_defaults(){
	## Arrays 
	[ ! ${STATUS} ] && STATUS=()								# init STATUS if it doesn't exist 
	#  pre-allocate variables here to fill later
	RECONSTATS=() 												# array of status outputs across subjects
	FINISHEDSUBJECTS=() && FINISHTIMES=() && STARTTIMES=() 		# array of subjects, completion times and start times of finished jobs
	RUNNINGSUBJECTS=() && RUNNINGSTATUS=() && RUNNINGTIME=() 	# array of subjects, statuses and last reported run time of active jobs
	FAILEDSUBJECTS=() && FAILTIMES=() && FAILPOINT=()			# array of subjects, exit times and last reported step of failed jobs
	RUNNINGHOSTS=() && FAILEDHOSTS=() && FINISHEDHOSTS=()
	## stats arrays for table detail
	status=() && hname=() && talqa=() && bcnr=() && acnr=()
	ndef=() && leno=() && reno=() && bmedits=() && runtime=()
	wmerased=() && wmfilled=()	
	## manifest check list
	MISSING=() && EXISTS=() && RERUNSUB=() && COMPLETESUB=() 	# array for subjects with missing/existing/complete data or those needing rerun
	## Pretty Colors
	[ ! ${PRETTY} ] && PRETTY='paintitpretty'
	#  make things pretty by default unless user asks
	if [ ${PRETTY} = 'paintitpretty' ]; then
		RED='\033[0;31m'
		BRED='\033[1;31m'
		YELLOW='\033[1;33m'
		GREEN='\033[1;32m'
		BLUE='\033[1;34m'
		GREY='\033[1;37m'
		NC='\033[0m' 
		BBLUE='\033[1;40m'
		NBC='\033[49m'
		GGREY='\033[1;30m'
		YBC='\033[47m'
		LRED='\033[1;31m'
	else
		RED='' && BRED='' && YELLOW=''
		GREEN='' && BLUE='' && GREY=''
		NC='' && BBLUE='' && NBC=''
		GGREY='' && YBC='' && LRED=''
	fi	
	## Table Preallocation
	TAB=$'\t' && TABLE=("SubjectID${TAB}Machine${TAB}JobStatus${TAB}TotalRunTime${TAB}ReconStage${TAB}NumBrainMaskEdits${TAB}NumWhiteMaskDel${TAB}NumWhiteMaskFill${TAB}TalCorr${TAB}OrigCNR${TAB}BrainMaskCNR${TAB}NumDefects${TAB}LEulerNo${TAB}REulerNo\n")
	# HTML Web Report
	REPORTREPO_TEMPLATE="https://github.com/seldamat/Surfer-webgems.git"
	REPORTREPO=''
	[ ! "${REPORTDIR}" ] && REPORTDIR="${HOME}/Surfer-webgems"	
	## Files for Webreports
	SURFs='lh.pial lh.white rh.pial rh.white'
	MGZs='aparc.DKTatlas+aseg T1 brainmask wm wmparc ribbon edits aseg aparc+aseg aparc.a2009s+aseg'	
}
