## DEFINE FUNCTION
### Print verbose status
function check-recons_printstatus_verbose(){
	if [ "${RUNNINGSUBJECTS[*]}" ]; then
		echo 'Active Job Status Details ::'		
		if [ ${VERBOSE} ]; then 
			for SUBJECT in ${RUNNINGSUBJECTS[@]}; do
				echo -e "\n${BLUE}${SUBJECT}\n${GREY}$(tail -n 1 $(find ${SUBJECTS_DIR}/${SUBJECT} -name 'recon-all-status.log'))${GGREY}\n$(tail -n 4 $(find ${SUBJECTS_DIR}/${SUBJECT} -name 'recon-all.log'))${NC}\n"
			done		
		fi	
	fi
	if [ "${FINISHEDSUBJECTS[*]}" ]; then
		echo 'Completed Job Status Details ::'		
		if [ ${VERBOSE} ]; then
			for SUBJECT in ${FINISHEDSUBJECTS[@]}; do
				echo -e "\n${BLUE}${SUBJECT}\n${GREEN}$(tail -n 1 $(find ${SUBJECTS_DIR}/${SUBJECT} -name 'recon-all-status.log'))${GGREY}\n$(tail -n 4 $(find ${SUBJECTS_DIR}/${SUBJECT} -name 'recon-all.log')| head -n 5 )${NC}\n"
			done		
		fi
	fi
	if [ "${FAILEDSUBJECTS[*]}" ]; then
		echo 'Failed Job Status Details ::'		
			for SUBJECT in ${FAILEDSUBJECTS[@]}; do
				echo -e "\n${BLUE}${SUBJECT}\n${BRED}$(tail -n 1 $(find ${SUBJECTS_DIR}/${SUBJECT} -name 'recon-all-status.log'))${GGREY}\n$(tail -n 8 $(find ${SUBJECTS_DIR}/${SUBJECT} -name 'recon-all.log'))${NC}\n"
			done		
	fi		
}
## DEFINE FUNCTION
### Print status
function check-recons_printstatus(){
	# print status of active jobs
	## TODO modify job submission script to write file to SUBJECTS_DIR/SUBJECT/script with time recon started in SGE
	if [ "${RUNNINGSUBJECTS[*]}" ]; then
		counter=0 && status=""
		for SUBJECT in ${RUNNINGSUBJECTS[@]}; do
			tstart=${RUNNINGTIME[$counter]} && tnow=$(date) && tstart=$(date -d "${tstart}" '+%s') && tnow=$(date -d "${tnow}" '+%s')
			trun=$((tnow-tstart)) && runtime=$(printf '%02d:%02d:%02d\n' $(($trun / 3600)) $(($trun % 3600 / 60)) $(($trun % 60)))
			status+=$(echo "\n${GREY}${RUNNINGHOSTS[$counter]}\t${BLUE}${SUBJECT}\t${GREY}${RUNNINGSTATUS[$counter]}\t${GREY}${runtime}${NC}")
			((counter++))
		done
		status=$(echo -e ${status} | sort -k1 -r)		
		header=$(echo -e "\n${GGREY}HOST\t${GGREY}SUBJECTID\t${GGREY}STATUS\t${LRED}TIME-ELAPSED${NC}\n")
		echo -e "🏃  ${GREY}${#RUNNINGSUBJECTS[@]} Current Active Jobs Running On Hosts :: ${NC}\n"
		[ $(which qstat) ] && printf  "    Remaining Subjects ::: %d\n\n" $(qstat -s p | grep 'qw' | wc -l)
		echo -e "${header}\n${status}" |  column -s $'\t' -t  && echo -e '\n' &&  [ $(which qhost) ] && qhost | sed '3d;' && echo -e '\n See more information by typing qstat -f -ne\n'
	fi
	# print status of finished jobs
	if [ "${FINISHEDSUBJECTS[*]}" ]; then
		counter=0 && status=""
		for SUBJECT in ${FINISHEDSUBJECTS[@]}; do
			[ "$(echo ${RERUNSUB[@]} | grep "${SUBJECT}")" ] && rerun="${LRED}missing files"
			[ "$(echo ${COMPLETESUB[@]} | grep "${SUBJECT}")" ] && rerun="${GREEN}complete"
			tstart=${STARTTIMES[$counter]} && tdone=${FINISHTIMES[$counter]}
			tdonep=$(date -d "${tdone}" '+%s') && tstart=$(date -d "${tstart}" '+%s') && trun=$((tdonep-tstart))
			runtime=$(printf '%02d:%02d:%02d\n' $(($trun / 3600)) $(($trun % 3600 / 60)) $(($trun % 60)))
			status+=$(echo "\n${GREY}${FINISHEDHOSTS[$counter]}\t${BLUE}${SUBJECT}\t${GREY}${tdone}\t${GREY}${runtime}\t${rerun}${NC}")
			((counter++))
		done
		status=$(echo -e ${status} | sort -k1 -r)
		header=$(echo -e "\n${GGREY}HOST\t${GGREY}SUBJECTID\t${GGREY}COMPLETION-TIME\t${GGREY}TIME-ELAPSED\tDATASET-MANIFEST\n")
		echo -e "✅ ${GREEN}${#FINISHEDSUBJECTS[@]} Completed Jobs :: ${NC}\n"
		echo -e "${header}\n${status}" | column -s $'\t' -t && echo -e '\n'
	fi
	# print status of failed subjects
	if [ "${FAILEDSUBJECTS[*]}" ]; then
		counter=0 && status=""
		for SUBJECT in ${FAILEDSUBJECTS[@]}; do
			status+=$(echo "\n${GREY}${FAILEDHOSTS[$counter]}\t${BRED}${SUBJECT}\t${YELLOW}${FAILPOINT[$counter]}\t${LRED}${FAILTIMES[$counter]}${NC}")
			((counter++))
		done
		status=$(echo -e ${status} | sort -k1 -r)
		header=$(echo -e "\n${GGREY}HOST\t${GGREY}SUBJECTID\t${GGREY}RECON-STEP\t${GGREY}TIME-FAILED\n")
		echo -e "🔥 ${YELLOW}${#FAILEDSUBJECTS[@]} Failed Jobs${NC} :: \n"
		echo -e "${header}\n${status}" | column -s $'\t' -t && echo -e '\n'
	fi
	[ "$(echo "${RERUNSUB[@]}")" ] && echo -e "\n${LRED}Resubmit the following jobs (incomplete file manifest):${NC}" && printf "${FREESURFER_HOME}/recon-all -s %s -make all\n" ${RERUNSUB[@]}
	[ "$(echo "${COMPLETESUB[@]}")" ] && echo -e "\n${GREEN}Complete file manifests (ready for analysis):${NC}" && printf "${BLUE}%s${NC} " ${COMPLETESUB[@]}
}
## DEFINE FUNCTION
### Print stats to table
function check-recons_table(){
	local counter=0
	echo -e '\r\n'
	for SUBJECT in ${SUBJECTS[@]}; do
		printf "📝  $counter/${#SUBJECTS[*]} -- Building Table\r"
		TABLE+=($(echo "${SUBJECT}\t${hname[$counter]}\t${status[$counter]}\t${runtime[$counter]}\t${reconstatus[$counter]}\t${bmedits[$counter]}\t${wmfilled[$counter]}\t${wmerased[$counter]}\t${talqa[$counter]}\t${bcnr[$counter]}\t${acnr[$counter]}\t${ndef[$counter]}\t${leno[$counter]}\t${reno[$counter]}\n"))
		((counter++))
	done
	echo -e "${TABLE[@]}" > ${SUBJECTS_DIR}/check-recons-report-$(date '+%H:%M:%S|%Y-%m-%d').txt && echo -e '\n 💾 Table written to'${SUBJECTS_DIR}'/check-recons-report.txt\n'
	cat ${SUBJECTS_DIR}/check-recons-report-$(date '+%H:%M:%S|%Y-%m-%d').txt | column -s $'\t' -t
}
## DEFINE FUNCTION
### Convert file into NIFTI
function check-recons_convert.NIFTI(){
	[ ! "${EXISTS}" ] && check-recons_files
	# echo -e '\nRequired Image/Surface Files for Web Report'
	echo 'Files Required for Report...'
	printf '\t%s.nii.gz\n' $(echo ${MGZs} ${SURFs})
	echo ''
	for SUBJECT in ${SUBJECTS[@]}; do
		for MGZ in ${MGZs}; do
			#&& [ "$(echo $(printf '%s %s\n' ${EXISTS[@]} | grep "${SUBJECT}" | grep -oE "${MGZ}.mgz"))" ] && 
			[ ! -f " ${SUBJECTS_DIR}/${SUBJECT}/mri/${MGZ}.nii.gz" ] && mri_convert --out_type nii ${SUBJECTS_DIR}/${SUBJECT}/mri/${MGZ}.mgz  ${SUBJECTS_DIR}/${SUBJECT}/mri/${MGZ}.nii.gz 			
		done
		for SURF in ${SURFs}; do
			[ ! -f " ${SUBJECTS_DIR}/${SUBJECT}/surf/${SURF}.gii" ]  &&	[ "$(echo $(printf '%s %s \n' ${EXISTS[@]} | grep "${SUBJECT}" | grep -oE "${SURF}"))" ] && mris_convert ${SUBJECTS_DIR}/${SUBJECT}/surf/${SURF} ${SUBJECTS_DIR}/${SUBJECT}/surf/${SURF}.gii 
		done
	done	
}

